echo "Testing avg filter"
python picturediff.py  filtered-avg-boats.bmp  ./originalFilterout/filtered-avg-boats.bmp
python picturediff.py  filtered-avg-blocks-small.bmp  ./originalFilterout/filtered-avg-blocks-small.bmp

echo "Testing hline filter"
python picturediff.py  filtered-hline-boats.bmp  ./originalFilterout/filtered-hline-boats.bmp
python picturediff.py  filtered-hline-blocks-small.bmp  ./originalFilterout/filtered-hline-blocks-small.bmp

